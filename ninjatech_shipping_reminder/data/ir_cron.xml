<odoo>

    <record id="ir_cron_stock_picking_tracking_notification" model="ir.cron">
        <field name="name">Tracking Notification Email</field>
        <field name="model_id" ref="ninjatech_shipping_reminder.model_stock_picking" />
        <field name="state">code</field>
        <field name="code">model.ir_cron_stock_picking_done()</field>
